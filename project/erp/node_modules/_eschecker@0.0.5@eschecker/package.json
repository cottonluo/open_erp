{
  "name": "eschecker",
  "version": "0.0.5",
  "description": "Type Checker for ECMAScript",
  "preferGlobal": true,
  "bin": {
    "eschecker": "index.js"
  },
  "directories": {
    "example": "examples",
    "doc": "doc",
    "lib": "lib"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "scripts": {
    "start": "gulp watch",
    "prepublish": "gulp default",
    "build": "gulp default",
    "test": "mocha --inline-diffs --compilers js:babel-register './test/**/*.spec.js'",
    "testWatch": "mocha --watch --compilers js:babel-register --watch-extensions dot './test/**/*.spec.js'",
    "coverage": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- './test/**/*.spec.js'",
    "coveralljs": "npm run coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js && rm -rf coverage",
    "esdoc": "esdoc -c .esdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DatenMetzgerX/ESChecker.git"
  },
  "author": "Micha Reiser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DatenMetzgerX/ESChecker/issues"
  },
  "homepage": "https://github.com/DatenMetzgerX/ESChecker#readme",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-node5": "^11.0.1",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "del": "^2.2.0",
    "esdoc": "^0.4.6",
    "eslint": "^2.8.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "node-babel": "^0.1.2",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "babel-code-frame": "^6.7.7",
    "babel-generator": "^6.7.7",
    "babel-traverse": "^6.7.6",
    "babel-types": "^6.7.7",
    "babylon": "^6.7.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "immutable": "^3.8.1",
    "lodash": "^4.11.1",
    "glob": "^7.0.3",
    "graphviz": "0.0.8",
    "pkginfo": "^0.4.0",
    "resolve": "^1.1.7"
  },
  "optionalDependencies": {
    "babel-runtime": "^6.6.1"
  },
  "_from": "eschecker@0.0.5",
  "_resolved": "http://registry.npm.taobao.org/eschecker/download/eschecker-0.0.5.tgz"
}